<div class="difficulty-container">
    <span class="difficulty-label">Difficulté</span>

    <div class="stars">
        @for (int i = 1; i <= 10; i++)
        {
            <img src="Icons/Star.png" class="star @(i > Difficulty ? "inactive" : "")" />
        }
    </div>
</div>

@code {
	[Parameter] public int Difficulty { get; set; } = 5;
}
